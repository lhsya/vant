#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamliu/works/vant/packages/vant-cli/node_modules:/Users/adamliu/works/vant/packages/node_modules:/Users/adamliu/works/vant/node_modules:/Users/adamliu/works/node_modules:/Users/adamliu/node_modules:/Users/node_modules:/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adamliu/works/vant/packages/vant-cli/node_modules:/Users/adamliu/works/vant/packages/node_modules:/Users/adamliu/works/vant/node_modules:/Users/adamliu/works/node_modules:/Users/adamliu/node_modules:/Users/node_modules:/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vant/cli/bin.js" "$@"
else
  exec node  "$basedir/../@vant/cli/bin.js" "$@"
fi
