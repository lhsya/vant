#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/bin/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/bin/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/dist/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules/transliteration/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/transliteration@2.3.5/node_modules:/Users/adamliu/works/vant/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/transliterate" "$@"
else
  exec node  "$basedir/../../dist/bin/transliterate" "$@"
fi
